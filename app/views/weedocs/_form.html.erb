<%= form_with(model: @weedoc, url: weedocs_add_path) do |form| %>


  <div class="field">
    <%= form.label :category, "대분류" %>
    <%= form.text_field :category %>
  </div>

  <div class="field">
    <%= form.label :organ, "기관" %>
    <%= form.text_field :organ %>
  </div>

  <div class="field">
    <%= form.label :sign1, "증상1" %>
    <%= form.text_field :sign1 %>
  </div>

  <div class="field">
    <%= form.label :sign2, "증상2" %>
    <%= form.text_field :sign2 %>
  </div>
  
  <div class="field">
    <%= form.label :strong, "강도" %>
    <%= form.text_field :strong %>
  </div>

  <div class="field">
    <%= form.label :cause, "원인" %>
    <%= form.text_field :cause %>
  </div>

  <div class="field">
    <%= form.label :expect, "예상" %>
    <%= form.text_field :expect %>
  </div>


  <div class="actions">
    <%= form.submit "작성(作成)"%>
  </div>
<% end %>
